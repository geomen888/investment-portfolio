#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yevgeniygeyman/PROJECTS/investment-portfolio/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@20.16.11_typescript@5.6.3_/node_modules/typeorm/node_modules:/Users/yevgeniygeyman/PROJECTS/investment-portfolio/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@20.16.11_typescript@5.6.3_/node_modules:/Users/yevgeniygeyman/PROJECTS/investment-portfolio/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yevgeniygeyman/PROJECTS/investment-portfolio/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@20.16.11_typescript@5.6.3_/node_modules/typeorm/node_modules:/Users/yevgeniygeyman/PROJECTS/investment-portfolio/backend/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@20.16.11_typescript@5.6.3_/node_modules:/Users/yevgeniygeyman/PROJECTS/investment-portfolio/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@20.16.11_typescript@5.6.3_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.20_pg@8.13.0_ts-node@10.9.2_@types+node@20.16.11_typescript@5.6.3_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
fi
